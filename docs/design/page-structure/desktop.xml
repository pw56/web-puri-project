<WebPage>
  
  <Start>
    <Title>
      <Image name="sample-1"/>
      <Image name="sample-2"/>
      <Object name="heart-1"/>
      <Object name="heart-2"/>
      <Text name="title-name" type="plane">Webプリ</Text>
    </Title>
    <Button name="shooting" type="button-1">撮影</Button>
    <Button name="puri-shooting" type="button-1">プリモード</Button>
    <Button name="upload" type="button-1">アップロード</Button>
  </Start>

  <QuantitySelection>
    <Text name="page-title" type="plane">枚数選択</Text>
    <Quantity>
      <Text name="count" type="value"></Text>
      <Button name="add" type="button-1" image=""></Button>
      <Button name="subtract" type="button-1" image=""></Button>
    </Quantity>
    <Button name="start-shooting" type="button-1">撮影を始める</Button>
  </QuantitySelection>

  <Shoot>
    <Text name="n枚目"/>
    <Shooting>
      <Video name="camera"/>
      <Button name="shoot" type="button-2"/>
      <Button name="switch" type="button-1" image="camera-switch.svg"/>
    </Shooting>
    <Checking>
      <Image name="check"/>
      <Button name="retry" type="button-1">撮り直し</Button>
    </Checking>
  </Shoot>

  <PuriShoot>
    <Text name="n枚目"/>
    <Shooting>
      <Video name="camera"/>
      <Pose>
        <Image name="example" image=""/>
        <Text name="message" type="plane"/>
      </Pose>
      <Countdown>
        <Object name="background-circle"/>
        <Text name="number" type="plane"/>
      </Countdown>
    </Shooting>
    <Checking>
      <Image name="check" src=""/>
      <Button name="retry" type="button-1">撮り直し</Button>
    </Checking>
  </PuriShoot>

  <!--ここ(edit)から。このページ構造とマークダウン作って。Codespacesのtreeの方がタイパいい。おやすみ。 2025/09/01 21:52-->
  <Edit>
    <Result>
      <Canvas name="background"/>
      <Canvas name="photo"/>
      <Canvas name="drawing"/>
    </Result>

    <Function>
      <Select name="mode-switch" type="pink">
        <Option name="face" text="顔"/>
        <Option name="pen" text="ペン"/>
        <Option name="background" text="背景"/>
        <Option name="filter" text="フィルター"/>
      </Select>

      <Face>
        <Range name="eyes" min="0" max="10"/>
        <Range name="nose" min="0" max="10"/>
        <Range name="mouth" min="0" max="10"/>
        <Range name="eyebags" min="0" max="10"/>
        <Range name="eyebrows" min="0" max="10"/>
        <Range name="contour" min="0" max="10"/>
        <Range name="smooth-skin" min="0" max="10"/>
        <Range name="clear-skin" min="0" max="10"/>
      </Face>

      <Pen>
        <Range name="width" min="0" max="10">太さ</range>

        <Select name="color-palette" type="color" column="入れておいて" row="入れておいて">
          <Option name="pink-1" color="#ffdef7"/>
          <Option name="red-1" color="#ffcccc"/>
          <Option name="yellow-1" color="#fdfaa3"/>
          <Option name="green-1" color="#d0faa2"/>
          <Option name="blue-1" color="#c8f0ff"/>
          <Option name="purple-1" color="#e2d5fb"/>
          <Option name="black-1" color="#ffffff"/>

          <Option name="pink-2" color="#ffa7e9"/>
          <Option name="red-2" color="#fdb85f"/>
          <Option name="yellow-2" color="#fff15b"/>
          <Option name="green-2" color="#9df3d8"/>
          <Option name="blue-2" color="#77d9fd"/>
          <Option name="purple-2" color="#db87dc"/>
          <Option name="black-2" color="#d9d9d9"/>

          <Option name="pink-3" color="#ff7da6"/>
          <Option name="red-3" color="#ff7e1f"/>
          <Option name="yellow-3" color="#e5c5a6"/>
          <Option name="green-3" color="#62cc62"/>
          <Option name="blue-3" color="#5984ff"/>
          <Option name="purple-3" color="#ac6df2"/>
          <Option name="black-3" color="#999797"/>

          <Option name="pink-4" color="#ff2ea4"/>
          <Option name="red-4" color="#f42534"/>
          <Option name="yellow-4" color="#8c4b25"/>
          <Option name="green-4" color="#437434"/>
          <Option name="blue-4" color="#3339ad"/>
          <Option name="purple-4" color="#7935b7"/>
          <Option name="black-4" color="#242424"/>
        </Select>

        <Select name="pen-switch" type="menu" max-column="3">
          <Option name="normal" image="" text="ノーマル"/>
          <Option name="narrow" image="" text="ほそ"/>
          <Option name="white-border" image="" text="白ふち"/>
          <Option name="black-border" image="" text="黒ふち"/>
          <Option name="loose" image="" text="ゆる"/>
          <Option name="white-neon" image="" text="白ネオン"/>
          <Option name="black-neon" image="" text="黒ネオン"/>
          <Option name="plump" image="" text="ぷっくり"/>
          <Option name="plump-shadow" image="" text="ぷっくりカゲ"/>
          <Option name="crayon" image="" text="クレヨン"/>
          <Option name="translucent" image="" text="半透明"/>
          <Option name="translucent-shadow" image="" text="半透明ふち"/>
          <Option name="blur" image="" text="ぼかし"/>
        </Select>
      </Pen>

      <Background>
        <Select type="menu">
          <Option name="example-1" image="" text=""/>
          <Option name="example-2" image="" text=""/>
          <Option name="example-3" image="" text=""/>
        </Select>
      </Background>

      <Filter>
        <Select type="menu">
          <Option name="example-hot" image="" text="Example hot"/>
          <Option name="example-dry" image="" text="Example dry"/>
          <Option name="example-mono" image="" text="Example mono"/>
        </Select>
      </Filter>
    </Function>
  </Edit>

  <Output>
    <Image name="preview"/>
    <Button name="download" type="button-1">ダウンロード</Button>
    <Button name="share" type="button-1">シェアする</Button>
    <Button name="print" type="button-1">プリント</Button>
    <Button name="end" type="button-1">終わる</Button>
  </Output>
  
</WebPage>