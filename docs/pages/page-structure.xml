<App>
  <StartPage/>
  <QuantitySelectPage/>
  <ShootPage/>
  <PuriShootPage/>

  <!--ここ(edit)から。このページ構造とマークダウン作って。Codespacesのtreeの方がタイパいい。おやすみ。 2025/09/01 21:52-->
  <EditPage>

    <Banner>
      <Button name="back" type="back"/>
      <Text name="frame-index" type="value"/>
      <Button name="next" type="next"/>
    </Banner>

    <Result>
      <Canvas name="background"/>
      <Canvas name="photo"/>
      <Canvas name="drawing"/>
    </Result>

    <Workspace>
      <Select name="mode-switch" type="pink">
        <Option name="face" text="顔"/>
        <Option name="pen" text="ペン"/>
        <Option name="stamp" text="スタンプ"/>
        <Option name="background" text="背景"/>
      </Select>

      <Face>
        <Text>目</Text><Range name="eyes" min="0" max="10"/>
        <Text>鼻</Text><Range name="nose" min="0" max="10"/>
        <Text>口</Text><Range name="mouth" min="0" max="10"/>
        <Text>涙袋</Text><Range name="eyebags" min="0" max="10"/>
        <Text>眉毛</Text><Range name="eyebrows" min="0" max="10"/>
        <Text>透明肌</Text><Range name="clear-skin" min="0" max="10"/>
        <Text>美肌</Text><Range name="smooth-skin" min="0" max="10"/>
        <Text>輪郭</Text><Range name="contour" min="0" max="10"/>
      </Face>

      <Pen>
        <Range name="width" min="0" max="10">太さ</range>

        <Select name="color-palette" type="color" column="7" row="4">
          <Option name="pink-1" color="#ffdef7"/>
          <Option name="red-1" color="#ffcccc"/>
          <Option name="yellow-1" color="#fdfaa3"/>
          <Option name="green-1" color="#d0faa2"/>
          <Option name="blue-1" color="#c8f0ff"/>
          <Option name="purple-1" color="#e2d5fb"/>
          <Option name="black-1" color="#ffffff"/>

          <Option name="pink-2" color="#ffa7e9"/>
          <Option name="red-2" color="#fdb85f"/>
          <Option name="yellow-2" color="#fff15b"/>
          <Option name="green-2" color="#9df3d8"/>
          <Option name="blue-2" color="#77d9fd"/>
          <Option name="purple-2" color="#db87dc"/>
          <Option name="black-2" color="#d9d9d9"/>

          <Option name="pink-3" color="#ff7da6"/>
          <Option name="red-3" color="#ff7e1f"/>
          <Option name="yellow-3" color="#e5c5a6"/>
          <Option name="green-3" color="#62cc62"/>
          <Option name="blue-3" color="#5984ff"/>
          <Option name="purple-3" color="#ac6df2"/>
          <Option name="black-3" color="#999797"/>

          <Option name="pink-4" color="#ff2ea4"/>
          <Option name="red-4" color="#f42534"/>
          <Option name="yellow-4" color="#8c4b25"/>
          <Option name="green-4" color="#437434"/>
          <Option name="blue-4" color="#3339ad"/>
          <Option name="purple-4" color="#7935b7"/>
          <Option name="black-4" color="#242424"/>
        </Select>

        <Select name="pen-switch" type="menu" max-column="3">
          <!--ここはプログラムにより動的生成-->
          <Option name="normal" image="" text="ノーマル"/>
          <Option name="narrow" image="" text="ほそ"/>
          <Option name="white-border" image="" text="白ふち"/>
          <Option name="black-border" image="" text="黒ふち"/>
          <Option name="loose" image="" text="ゆる"/>
          <Option name="white-neon" image="" text="白ネオン"/>
          <Option name="black-neon" image="" text="黒ネオン"/>
          <Option name="plump" image="" text="ぷっくり"/>
          <Option name="plump-shadow" image="" text="ぷっくりカゲ"/>
          <Option name="crayon" image="" text="クレヨン"/>
          <Option name="translucent" image="" text="半透明"/>
          <Option name="translucent-shadow" image="" text="半透明ふち"/>
          <Option name="blur" image="" text="ぼかし"/>
        </Select>
      </Pen>

      <Background>
        <Select type="menu">
          <Option name="example-1" image="" text=""/>
          <Option name="example-2" image="" text=""/>
          <Option name="example-3" image="" text=""/>
        </Select>
      </Background>
    </Workspace>
  </EditPage>

  <ResultPage>
    <Button name="end" type="button-1">終わる</Button>
    <Scroll type="puri">
      <!--ここはプログラムにより動的生成-->
      <Image name="preview-1"/>
      <Image name="preview-2"/>
      <Image name="preview-3"/>
      <Image name="preview-4"/>
    </Scroll>
    <!--デスクトップ版-->
      <Button name="download" type="button-1"></Button>
    <!--モバイル版-->
      <Button name="share" type="button-1"></Button>
    <Button name="print" type="button-1"></Button>
    <Button name="location" type="button-1"></Button>
    <Tag name="tags"></Tag>
  </ResultPage>
  
  <PrintPage>
    <Image name="preview"/>
    <Workspace>
      <Select name="mode-switch" type="pink">
        <Option name="frame" text="フレーム"/>
        <Option name="panel-layout" text="コマ割り"/>
        <Option name="size" text="サイズ"/>
      </Select>
      <Frame>
        <Select name="menu" type="menu" max-column="3">
          <!--ここはプログラムにより動的生成-->
        </Select>
      </Frame>
      <PanelLayout>
        <Column>
          <Text type="plain">横</Text>
          <Count/> <!--"default-value"属性をプログラムから動的設定-->
        </Column>
        <Row>
          <Text type="plain">縦</Text>
          <Count/> <!--"default-value"属性をプログラムから動的設定-->
        </Row>
      </PanelLayout>
      <Size>
        <SheetSize>
          <Text name="property" type="plain">用紙</Text>
          <Select name="size-select" type="white">
            <Option name="" text="L判"/>
            <Option name="" text="2L判"/>
            <Option name="" text="A4"/>
            <Option name="" text="B5"/>
          </Select>
        </SheetSize>

        <PrintSize>
          <Text name="property" type="plain">印刷サイズ</Text>
          <Select name="size-select" type="white">
            <Option name="puri-size" text="プリサイズ"/>
            <Option name="zoom" text="拡大"/>
          </Select>
        </PrintSize>
      </Size>
      <Object name="background" type="">
    </Workspace>
    <Button name="cancel-printing" type="back"/>
    <Button name="start-printing" type="button-1">
  </PrintPage>

  <AlbumPage>
    <Button name="close-album" type="back"></Button>
    <Images>
      <!--ここはプログラムにより動的生成-->
    </Images>
    <Pages>
      <Button name="previous" type="back"/>
      <Text name="current-page" type="plain"/>
      <Button name="next" type="next"/>
    </Pages>
  </AlbumPage>

  <PickedPage>
    <Button name="back-to-album" type="back"/>
    <Button name="delete" type="button-1" image=""/>
    <Image name="preview"/>
    <Date>
      <Text name="property" type="plain"></Text>
      <Text name="link" type="link"></Text>
    </Date>
    <Place>
      <Text name="property" type="plain"></Text>
      <Text name="link" type="link"></Text>
    </Place>
  </PickedPage>

</App>